{
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "Social Networking",
    "shortDescription": "",
    "longDescription": "",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "build": {
    "beforeBuildCommand": "",
    "frontendDist": "../dist",
    "beforeDevCommand": ""
  },
  "productName": "Discord Tauri",
  "version": "0.3.0",
  "identifier": "discord.tauri",
  "plugins": {},
  "app": {
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'; connect-src ipc: http://ipc.localhost"
    },
    "windows": [
      {
        "title": "Discord Tauri",
        "width": 1200,
        "height": 650,
        "resizable": true,
        "minWidth": 940,
        "minHeight": 500,
        "fullscreen": false,
        "visible": false,
        "url": "index.html",
        "label": "main",
        "decorations": true,
        "center": true
      }
    ],
    "withGlobalTauri": true
  }
}